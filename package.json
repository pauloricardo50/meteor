{
  "name": "epotek",
  "private": true,
  "scripts": {
    "start": "meteor --settings settings-dev.json",
    "production": "meteor run --production --settings settings-dev.json",
    "visualize": "meteor --extra-packages bundle-visualizer --production --settings settings-dev.json --port 3010",
    "test": "meteor test --driver-package=practicalmeteor:mocha --settings settings-dev.json --port 3005",
    "test-debug": "meteor test --driver-package=practicalmeteor:mocha --settings settings-dev.json --port 5000 --debug-port 5858",
    "test-CI": "TEST_BROWSER_DRIVER=nightmare SERVER_TEST_REPORTER=xunit XUNIT_FILE=$CIRCLE_TEST_REPORTS/reports/unit.xml meteor test --settings settings-dev.json --once --driver-package dispatch:mocha",
    "test-cli": "TEST_BROWSER_DRIVER=nightmare meteor test --settings settings-dev.json --port 3002 --driver-package dispatch:mocha",
    "deploy:production": "cd .deploy/production && mup deploy --settings=settings-production.json",
    "deploy:staging": "cd .deploy/staging && mup deploy --settings=settings-staging.json",
    "clean": "rm -rf node_modules/ && meteor npm install",
    "cleanstart": "meteor npm run clean && meteor npm run start"
  },
  "version": "1.0.0",
  "description": "e-Potek main",
  "main": "index.js",
  "author": "florianbienefelt",
  "license": "ISC",
