{
  "name": "epotek",
  "version": "1.2.0",
  "scripts": {
    "clean-css": "cd scripts/ && ./clean-css.sh",
    "clean-run": "npm run setup-clean && npm run run",
    "deploy-production": "cd .deployment/ && npm run deploy-production",
    "deploy-staging": "cd .deployment/ && npm run deploy-staging",
    "flow-typed": "flow-typed install",
    "flow": "flow",
    "format": "npx eslint --fix .",
    "generate-circleci-configs": "babel-node .circleci/generateConfig.js",
    "lang": "babel-node scripts/createLanguages.js",
    "lint": "npx eslint .",
    "mongo-ssh-production": "cd .deployment/ && npm run mongo-ssh-production",
    "mongo-ssh-staging": "cd .deployment/ && npm run mongo-ssh-staging",
    "plop": "npx plop",
    "pull-production-db": "cd .deployment/ && npm run pull-production-db",
    "pull-staging-db": "cd .deployment/ && npm run pull-staging-db",
    "remove-core": "cd scripts/ && ./remove-core.sh",
    "restore-production-to-staging": "cd .deployment/ && npm run restore-production-to-staging",
    "run": "cd scripts/ && ./run.sh",
    "setup-clean": "cd scripts/ && ./setup.sh -c",
    "setup": "cd scripts/ && ./setup.sh",
    "start-maintenance-production": "cd .deployment/ && npm run start-maintenance-production",
    "start-maintenance-staging": "cd .deployment/ && npm run start-maintenance-staging",
    "stop-maintenance": "cd .deployment/ && npm run stop-maintenance",
    "test-all": "cd scripts/ && ./test-all.sh",
    "update-nginx": "cd .deployment/ && npm run update-nginx"
  },
  "disabled-husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-Potek/epotek.git"
  },
  "bugs": {
    "url": "https://github.com/e-Potek/epotek/issues"
  },
  "homepage": "https://github.com/e-Potek/epotek#readme",
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.3",
    "@cypress/browserify-preprocessor": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "change-case": "^3.1.0",
    "cypress": "3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.5.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.96.0",
    "flow-typed": "^2.5.1",
    "global": "^4.3.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-cmd": "^3.0.0",
    "plop": "^2.3.0",
    "prettier": "^1.16.4"
  }
}
