{
  "name": "epotek",
  "version": "1.2.0",
  "scripts": {
    "clean-run": "npm run setup-clean && npm run run",
    "deploy-production": "cd mup && node run-all -e prod --parallel deploy",
    "deploy-staging": "cd mup && node run-all -e staging --parallel deploy",
    "format-code": "cd scripts && ./format-code.sh",
    "generate-circleci-configs": "babel-node .circleci/generateConfig.js",
    "lang": "babel-node scripts/createLanguages.js",
    "lint": "npx eslint .",
    "mongo-shell-production": "cd mup && node mongo-shell -e production",
    "mongo-shell-staging": "cd mup && node mongo-shell -e staging",
    "plop": "npx plop",
    "pull-production-db": "cd mup && node pull-mongo -e prod --out /tmp/prod.archive && node restore-mongo -e local --archivePath /tmp/prod.archive",
    "pull-staging-db": "cd mup && node pull-mongo -e staging --out /tmp/staging.archive && node restore-mongo -e local --archivePath /tmp/staging.archive",
    "remove-core": "cd scripts/ && ./remove-core.sh",
    "restore-production-to-staging": "cd mup && node pull-mongo -e prod --out /tmp/prod.archive && node restore-mongo -e staging --archivePath /tmp/prod.archive",
    "run": "cd scripts/ && ./run.sh",
    "setup-clean": "cd scripts/ && ./setup.sh -c",
    "setup": "cd scripts/ && ./setup-all.sh",
    "test-all": "cd scripts/ && ./test-all.sh",
    "classify-cities": "cd scripts/gpsStats && node classifyCities"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-Potek/epotek.git"
  },
  "bugs": {
    "url": "https://github.com/e-Potek/epotek/issues"
  },
  "homepage": "https://github.com/e-Potek/epotek#readme",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@cypress/browserify-preprocessor": "^2.1.1",
    "@google-cloud/compute": "^1.2.0",
    "babel-eslint": "^10.0.3",
    "chalk": "^4.0.0",
    "change-case": "^4.1.1",
    "csscomb": "^4.3.0",
    "cypress": "^4.4.1",
    "digest-fetch": "^1.0.9",
    "ejs": "^3.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "esm": "^3.2.25",
    "global": "^4.4.0",
    "import-sort-cli": "^6.0.0",
    "mup": "1.5.0-beta.10",
    "mup-netdata": "^0.3.0",
    "node-cmd": "^3.0.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.3",
    "write-yaml": "^1.0.0",
    "yargs": "^15.1.0"
  }
}
