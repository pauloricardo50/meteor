{
  "name": "epotek",
  "version": "1.2.0",
  "scripts": {
    "clean-run": "npm run setup-clean && npm run run",
    "deploy-production": "cd deploy && node run-all -e prod --parallel deploy",
    "deploy-staging": "cd deploy && node run-all -e staging --parallel deploy",
    "format-code": "cd scripts && ./format-code.sh",
    "generate-circleci-configs": "node -r esm .circleci/generateConfig.js",
    "lang": "node -r esm scripts/createLanguages.js",
    "lint": "npx eslint .",
    "mongo-shell-production": "cd deploy && node mongo-shell -e production",
    "mongo-shell-staging": "cd deploy && node mongo-shell -e staging",
    "plop": "npx plop",
    "pull-production-db": "cd deploy && node pull-mongo -e prod --out /tmp/prod.archive && node restore-mongo -e local --archivePath /tmp/prod.archive",
    "pull-staging-db": "cd deploy && node pull-mongo -e staging --out /tmp/staging.archive && node restore-mongo -e local --archivePath /tmp/staging.archive",
    "remove-core": "cd scripts/ && ./remove-core.sh",
    "restore-production-to-staging": "cd deploy && node pull-mongo -e prod --out /tmp/prod.archive && node restore-mongo -e staging --archivePath /tmp/prod.archive",
    "run": "cd scripts/ && ./run.sh",
    "setup-clean": "cd scripts/ && ./setup.sh -c",
    "setup": "cd scripts/ && ./setup-all.sh",
    "test-all": "cd scripts/ && ./test-all.sh",
    "classify-cities": "cd scripts/gpsStats && node classifyCities"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-Potek/epotek.git"
  },
  "bugs": {
    "url": "https://github.com/e-Potek/epotek/issues"
  },
  "homepage": "https://github.com/e-Potek/epotek#readme",
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@cypress/browserify-preprocessor": "^3.0.1",
    "@google-cloud/compute": "^2.0.2",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "csscomb": "^4.3.0",
    "cypress": "^4.10.0",
    "cypress-file-upload": "^4.0.7",
    "digest-fetch": "^1.1.1",
    "ejs": "^3.1.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-meteor": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "esm": "^3.2.25",
    "global": "^4.4.0",
    "import-sort-cli": "^6.0.0",
    "mup": "^1.5.2-beta.3",
    "mup-deploy-notifications": "^0.3.0",
    "mup-netdata": "^0.4.0",
    "node-cmd": "^3.0.0",
    "plop": "^2.7.1",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "write-yaml": "^1.0.0",
    "yargs": "^15.4.1"
  }
}
