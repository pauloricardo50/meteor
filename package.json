{
  "name": "epotek",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier-eslint \"**/*.js*\" --write --eslint-config-path .eslintrc.js",
    "setup": "cd scripts/ && ./setup.sh",
    "setup-clean": "cd scripts/ && ./setup.sh -c",
    "run": "cd scripts/ && ./run.sh",
    "lang": "cd scripts/ && meteor babel-node createLanguages.js",
    "clean-run": "npm run setup-clean && npm run run",
    "clean-css": "cd scripts/ && ./clean-css.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-Potek/epotek.git"
  },
  "bugs": {
    "url": "https://github.com/e-Potek/epotek/issues"
  },
  "homepage": "https://github.com/e-Potek/epotek#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "cypress": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-meteor": "^5.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "global": "^4.3.2",
    "meteor-babel": "^7.0.0-beta.47",
    "prettier": "^1.12.1"
  }
}
