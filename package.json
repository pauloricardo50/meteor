{
  "name": "epotek",
  "private": true,
  "scripts": {
    "start": "meteor --settings settings-dev.json",
    "test": "mocha --compilers js:babel-core/register ./tests/*.js",
    "deploy": "mup deploy --settings=settings-production.json"
  },
  "version": "1.0.0",
  "description": "e-Potek main",
  "main": "index.js",
  "author": "florianbienefelt",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "fixed-data-table": "^0.6.3",
    "highcharts": "^5.0.7",
    "indexof": "0.0.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "is_js": "^0.9.0",
    "ismobilejs": "^0.4.0",
    "lodash": "^4.17.4",
    "lumber-cli": "^1.2.8",
    "mailchimp-api-v3": "^1.7.0",
    "material-ui": "^0.17.0",
    "meteor-node-stubs": "~0.2.5",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "query-string": "^4.3.2",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-countup": "^1.3.0",
    "react-dom": "^15.4.2",
    "react-dropzone-component": "^1.4.1",
    "react-komposer": "^2.0.0",
    "react-motion": "^0.4.7",
    "react-mounter": "^1.2.0",
    "react-router-dom": "^4.0.0",
    "react-scroll": "^1.4.7",
    "react-tap-event-plugin": "^2.0.1",
    "react-text-mask": "^1.0.0",
    "react-tooltip": "^3.2.7",
    "simpl-schema": "^0.2.2",
    "text-mask-addons": "^3.2.0",
    "tracker-component": "^1.3.21",
    "uniforms": "^1.15.0",
    "uniforms-material": "^1.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "selenium-webdriver": "^3.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false,
          "classes": true
        }
      ],
      "import/extensions": 0,
      "import/no-extraneous-dependencies": "off",
      "import/no-absolute-path": "off",
      "class-methods-use-this": 0,
      "no-absolute-path": 0
    }
  }
}
