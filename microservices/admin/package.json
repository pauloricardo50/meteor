{
  "name": "epotek-admin",
  "private": true,
  "scripts": {
    "prestart": "cd ../.. && npm run lang -- admin",
    "start": "meteor run --settings settings-dev.json --port 5000",
    "start-local-db":
      "MONGO_URL=mongodb://localhost:27017/epotek meteor run --settings settings-dev.json --port 5000",
    "start-e2e":
      "meteor npm run prestart && meteor --settings settings-dev.json --port 5015",
    "cypress-run": "cypress run --browser chrome",
    "cypress-open": "cypress open",
    "test":
      "meteor test --driver-package=practicalmeteor:mocha --settings settings-dev.json --port 5005",
    "test-CI":
      "SERVER_TEST_REPORTER=xunit XUNIT_FILE=~/app/results/unit-admin.xml TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --settings settings-dev.json",
    "test-e2e":
      "start-server-and-test start-e2e http://localhost:5015 cypress-open",
    "test-e2e-CI":
      "start-server-and-test start-e2e http://localhost:5015 cypress-run",
    "clean": "rm -rf node_modules/ && meteor npm install"
  },
  "dependencies": {
