    "meteor-node-stubs": "^0.3.3",
    "modernizr": "^3.6.0",
    "moment": "^2.22.0",
    "normalize-scss": "^7.0.1",
    "query-string": "^5.1.1",
    "rc-slider": "^8.6.1",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-countup": "^3.0.3",
    "react-dom": "^16.3.1",
