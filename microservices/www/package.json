{
  "name": "epotek-www",
  "private": true,
  "scripts": {
    "cypress-open": "../../node_modules/cypress/bin/cypress open",
    "cypress-run-CI": "../../node_modules/cypress/bin/cypress run --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha-multi-reporters-config.json",
    "cypress-run": "../../node_modules/cypress/bin/cypress run",
    "debug": "meteor npm run start -- --inspect",
    "ngrok": "cd ../../scripts/ && ./ngrok.sh 3000",
    "prestart": "cd ../.. && npm run lang -- www",
    "start-e2e": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-dev.json --port 3015",
    "start": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" DISABLE_WEBSOCKETS=1 QUALIA_ONE_BUNDLE_TYPE=modern meteor --settings settings-dev.json --port 3000",
    "test-CI": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" SERVER_TEST_REPORTER=xunit SERVER_MOCHA_OUTPUT=~/app/results/unit-server.xml CLIENT_MOCHA_OUTPUT=~/app/results/unit-client.xml TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package meteortesting:mocha --settings settings-dev.json",
    "test-console-once": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" meteor test --driver-package meteortesting:mocha --once --settings settings-dev.json  --port 3006",
    "test-console": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" QUALIA_ONE_BUNDLE_TYPE=modern TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --settings settings-dev.json  --port 3006",
    "test-e2e-CI": "start-server-and-test start-e2e http://localhost:3015 cypress-run-CI",
    "test-e2e-once": "start-server-and-test start-e2e http://localhost:3015 cypress-run",
    "test-e2e": "start-server-and-test start-e2e http://localhost:3015 cypress-open",
    "test": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" meteor test --driver-package meteortesting:mocha --settings settings-dev.json --port 3005",
    "visualize": "METEOR_PACKAGE_DIRS=\"packages:../../meteorPackages\" meteor --extra-packages bundle-visualizer --production --settings settings-dev.json --port 3010"
  },
