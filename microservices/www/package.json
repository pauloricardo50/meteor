{
  "name": "www",
  "private": true,
  "scripts": {
    "prestart": "cd ../.. && npm run lang -- www",
    "start": "DISABLE_WEBSOCKETS=1 meteor --settings settings-dev.json --port 3000",
    "start:win": "set NODE_OPTIONS=--max-old-space-size=4096&& set METEOR_OFFLINE_CATALOG=1&& set MONGO_URL=mongodb://localhost:27017/epotek&& meteor --settings settings-dev.json --port 3000",
    "start-e2e": "meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-dev.json --port 3015",
    "cypress-run-CI": "../../node_modules/cypress/bin/cypress run --browser chrome --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha-multi-reporters-config.json",
    "cypress-open": "../../node_modules/cypress/bin/cypress open",
    "test": "meteor test --driver-package=cultofcoders:mocha --settings settings-dev.json --port 3005",
    "test-CI": "SERVER_TEST_REPORTER=xunit XUNIT_FILE=~/app/results/unit.xml TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --settings settings-dev.json",
    "test-e2e": "meteor start-server-and-test start-e2e http://localhost:3015 cypress-open",
    "test-e2e-CI": "meteor start-server-and-test start-e2e http://localhost:3015 cypress-run-CI",
    "ngrok": "cd ../../scripts/ && ./ngrok.sh 3000",
    "debug": "meteor npm run start -- --inspect",
    "visualize": "meteor --extra-packages bundle-visualizer --production --settings settings-dev.json --port 3010"
  },
