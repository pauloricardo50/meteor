{
  "name": "epotek-brand-website",
  "private": true,
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "clean": "gatsby clean",
    "predeploy": "npm run clean && npm run build",
    "deploy": "netlifyctl deploy",
    "deploy-draft": "netlifyctl deploy --draft",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "local": "npx --node-arg '-r esm' npm run develop -- -H 0.0.0.0",
    "serve": "npx --node-arg '-r esm' gatsby serve -H 0.0.0.0",
    "start": "npx --node-arg '-r esm' ./scripts/start.js",
    "test": "jest --watch",
    "test-ci": "NODE_ENV=test jest --ci",
    "test-e2e": "npx --node-arg '-r esm' ./scripts/test-e2e.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/pro-light-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@prismicio/gatsby-source-prismic-graphql": "^3.6.10",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "gatsby": "^2.20.28",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-canonical-urls": "^2.3.10",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
    "gatsby-plugin-hotjar": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-material-ui": "^2.1.9",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sass": "^2.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "^2.4.1",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "highcharts": "8.1.0",
    "highcharts-react-official": "^3.0.0",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.17",
    "node-sass": "^4.14.0",
    "normalize-scss": "^7.0.1",
    "prismic-reactjs": "^1.3.1",
    "prismic-richtext": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-highcharts": "^16.1.0",
    "react-intl": "3.12.1",
    "react-loadable": "^5.5.0",
    "react-scroll": "^1.7.16",
    "react-share": "^4.2.0",
    "react-spring": "^8.0.27",
