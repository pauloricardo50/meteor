  "devDependencies": {
    "@testing-library/react": "^10.4.4",
    "babel-jest": "^26.1.0",
    "babel-preset-gatsby": "^0.5.2",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^26.1.0",
    "start-server-and-test": "^1.11.2"
  }
}
