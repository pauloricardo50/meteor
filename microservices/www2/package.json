{
  "name": "epotek-brand-website",
  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "gatsby": "^2.20.0",
    "gatsby-image": "^2.3.0",
    "gatsby-plugin-manifest": "^2.3.0",
    "gatsby-plugin-netlify": "^2.2.0",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-sass": "^2.2.0",
    "gatsby-plugin-sharp": "^2.5.0",
    "gatsby-source-contentful": "^2.2.0",
    "gatsby-source-filesystem": "^2.2.0",
    "gatsby-transformer-sharp": "^2.4.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1"
  },
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "clean": "gatsby clean",
    "predeploy": "npm run clean && npm run build",
    "deploy": "netlifyctl deploy",
    "deploy-draft": "netlifyctl deploy --draft",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "start": "npx --node-arg '-r esm' npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
