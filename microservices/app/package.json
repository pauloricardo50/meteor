{
  "name": "epotek-app",
  "private": true,
  "scripts": {
    "prestart": "cd ../.. && npm run lang -- app",
    "start": "meteor --settings settings-dev.json --port 4000",
    "start-local-db": "MONGO_URL=mongodb://localhost:27017/epotek meteor --settings settings-dev.json --port 4000",
    "start-e2e": "meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-dev.json --port 4015",
    "cypress-run-CI": "../../node_modules/cypress/bin/cypress run --browser chrome --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha-multi-reporters-config.json",
    "cypress-open": "../../node_modules/cypress/bin/cypress open",
    "production": "meteor run --production --settings settings-dev.json",
    "visualize": "meteor --extra-packages bundle-visualizer --production --settings settings-dev.json --port 4010",
    "test": "meteor test --driver-package cultofcoders:mocha --settings settings-dev.json --port 4005",
    "test-console": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --settings settings-dev.json  --port 4006",
    "test-debug": "meteor test --driver-package cultofcoders:mocha --settings settings-dev.json --port 5000 --debug-port 5858",
    "test-CI": "SERVER_TEST_REPORTER=xunit XUNIT_FILE=~/app/results/unit.xml TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --settings settings-dev.json",
    "test-e2e": "meteor start-server-and-test start-e2e http://localhost:4015 cypress-open",
    "test-e2e-CI": "meteor start-server-and-test start-e2e http://localhost:4015 cypress-run-CI",
    "deploy:production": "cd .deploy/production && mup deploy --settings settings-production.json",
    "deploy:staging": "cd .deploy/staging && mup deploy --settings settings-staging.json",
    "clean": "rm -rf node_modules/ && meteor npm install",
    "cleanstart": "meteor npm run clean && meteor npm run start",
    "debug": "meteor npm run start -- --inspect",
    "ngrok": "cd ../../scripts/ && ./ngrok.sh 4000"
  },
  "version": "0.1.0",
  "description": "e-Potek main",
  "main": "index.js",
  "author": "florianbienefelt",
  "license": "ISC",
