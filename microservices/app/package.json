{
  "name": "epotek-app",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules/ && meteor npm install",
    "cleanstart": "meteor npm run clean && meteor npm run start",
    "cypress-open": "../../node_modules/cypress/bin/cypress open",
    "cypress-run-CI": "../../node_modules/cypress/bin/cypress run --reporter mocha-multi-reporters --reporter-options configFile=cypress/mocha-multi-reporters-config.json",
    "cypress-run": "../../node_modules/cypress/bin/cypress run",
    "debug": "meteor npm run start -- --inspect",
    "deploy:production": "cd .deploy/production && mup deploy --settings settings-production.json",
    "deploy:staging": "cd .deploy/staging && mup deploy --settings settings-staging.json",
    "ngrok": "cd ../../scripts/ && ./ngrok.sh 4000",
    "prestart": "cd ../.. && npm run lang -- app",
    "production": "meteor run --production --settings settings-dev.json",
    "start-e2e": "meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-dev.json --port 4015",
    "start-local-db": "MONGO_URL=mongodb://localhost:27017/epotek meteor --settings settings-dev.json --port 4000",
    "start": "meteor --settings settings-dev.json --port 4000",
    "test-CI": "SERVER_TEST_REPORTER=xunit SERVER_MOCHA_OUTPUT=~/app/results/unit-server.xml CLIENT_MOCHA_OUTPUT=~/app/results/unit-client.xml TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package meteortesting:mocha --settings settings-dev.json",
    "test-console-once": "meteor test --driver-package meteortesting:mocha --once --settings settings-dev.json  --port 4006",
    "test-console": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --settings settings-dev.json  --port 4006",
    "test-debug": "meteor test --driver-package meteortesting:mocha --settings settings-dev.json --port 5000 --debug-port 5858",
    "test-e2e-CI": "start-server-and-test start-e2e http://localhost:4015 cypress-run-CI",
    "test-e2e-once": "start-server-and-test start-e2e http://localhost:4015 cypress-run",
    "test-e2e": "start-server-and-test start-e2e http://localhost:4015 cypress-open",
    "test": "meteor test --driver-package meteortesting:mocha --settings settings-dev.json --port 4005",
    "visualize": "meteor --extra-packages bundle-visualizer --production --settings settings-dev.json --port 4010"
  },
