    "@babel/runtime": "7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/pro-light-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "antd": "^3.9.3",
    "aws-sdk": "^2.320.0",
    "babel-polyfill": "^6.26.0",
    "base64topdf": "^1.1.8",
    "bcrypt": "2.0.1",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "faker": "^4.1.0",
    "file-saver": "^1.3.8",
    "indexof": "0.0.1",
    "intl-messageformat": "^2.2.0",
    "intl-relativeformat": "https://github.com/Floriferous/intl-relativeformat/tarball/master",
    "js-search": "^1.4.2",
    "json-logic-js": "^1.2.2",
    "lodash": "^4.17.11",
    "meteor-accounts-t9n": "^2.3.1",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "normalize-scss": "^7.0.1",
    "prop-types": "^15.6.2",
    "query-string": "5.1.1",
