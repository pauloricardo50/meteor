{
  "presets": ["meteor", "@babel/preset-flow"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "core": "./imports/core",
          "@ant-design/icons/lib/dist$": "./imports/core/utils/antIcons"
        }
      }
    ]
  ]
}
